{
    "extends": "./node_modules/electron-webpack/tsconfig-base.json",
    "compilerOptions": {
        "lib": ["es6", "dom"],
        "noImplicitAny": false,
        "noImplicitThis": true,
        "strictNullChecks": false,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "noUnusedLocals": false,
        "skipLibCheck": true,
        "jsx": "react"
    }
